{
    "name": "Feedbin Link Opener",
    "description": "Open Feedbin links in a background tab.",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["https://feedbin.com/*"],
        "js": ["content.js"]
    }],
    "commands": {
        "open-feedbin-link": {
            "description": "Open Feedbin link in a background tab.",
            "suggested_key": {
                "default": "Alt+V"
            }
        }
    }
}